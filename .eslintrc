{
  "extends": "airbnb-base",
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [ "builtin", "external", "internal", "parent", "sibling", "index" ],
        "newlines-between": "always"
      }
    ],
    "no-empty": [
      "error",
      { "allowEmptyCatch": true }
    ],
    "no-param-reassign": [
      "error",
      { "props": false }
    ],
    "no-underscore-dangle": [
      "error",
      { "allow": [ "_claim_names", "_claim_sources" ] }
    ]
  }
}
